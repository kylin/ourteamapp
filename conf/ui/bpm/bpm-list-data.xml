<?xml version="1.0" encoding="UTF-8"?>
<listdata>

	<select name="bpm.bpmDomainList" type="getBpmDomains"
		dataProperty="id" labelProperty="name">

	</select>

	<select name="bpm.stepTypeList" type="static">
		<option value="GENERAL_TYPE">普通</option>
	</select>

	<select name="bpm.stepList" type="getBpmStepDefines"
		dataProperty="id" labelProperty="stepName">
	</select>

	<select name="bpm.roleList" type="getBpmRoles" dataProperty="id"
		labelProperty="roleName">
	</select>

	<select name="bpm.fieldTypeList" type="static">
		<option value="text_field">文本</option>
		<option value="combo_field">单选</option>
		<option value="check_field">多选</option>
		<option value="date_field">日期</option>
		<option value="time_field">时间</option>
		<option value="group_field">分组</option>
	</select>

	<select name="bpm.fieldStatusList" type="static">
		<option value="MUST">必填</option>
		<option value="OPTIONAL">可选</option>
		<option value="READ_ONLY">只读</option>
	</select>
	
	<select name="bpm.processStatusList" type="static">
		<option value="STARTED">启动</option>
		<option value="CANCELED">已取消</option>
		<option value="FINISHED">已完成</option>
	</select>
	
	<select name="bpm.taskPriorityList" type="static">
		<option value="50">中</option>
		<option value="100">高</option>
		<option value="0">低</option>
	</select>
	
	<select name="bpm.taskAssignerList" type="getBpmTaskCandidateAssigners"
		dataProperty="id" labelProperty="">
	</select>
	
	<select name="bpm.starterDepartmentList" type="getEmployeeDepartments"
		dataProperty="id" labelProperty="">
	</select>
	
	<select name="bpm.starterTeamList" type="getEmployeeTeams"
		dataProperty="id" labelProperty="">
	</select>
	
	

</listdata>
