<?xml version='1.0' encoding='UTF-8'?>
<actions>

	<action type="push" handler="" desc="Add New Server"
		icon="server_add" id="addNewServer" confirmInfoKey=""
		label="添加服务" labelKey="">
	</action>

	<actiongroup id="databaseServerActionGroup">
		<action ref="delete" handler="deleteDataBaseServer"></action>
	</actiongroup>

	<actiongroup id="cvsServerActionGroup">
		<action ref="delete" handler="deleteCVSServer"></action>
	</actiongroup>

	<actiongroup id="svnServerActionGroup">
		<action ref="delete" handler="deleteSVNServer"></action>
	</actiongroup>

	<actiongroup id="appServerActionGroup">
		<action ref="delete" handler="deleteAppServer"></action>
	</actiongroup>

	<actiongroup id="databaseServerTypeActionGroup">
		<action ref="addNewServer" handler="addNewDataBaseServer"
			label="添加数据库服务"></action>
	</actiongroup>

	<actiongroup id="cvsServerTypeActionGroup">
		<action ref="addNewServer" handler="addNewCVSServer" label="添加CVS服务"></action>
	</actiongroup>

	<actiongroup id="svnServerTypeActionGroup">
		<action ref="addNewServer" handler="addNewSVNServer" label="添加SVN服务"></action>
	</actiongroup>

	<actiongroup id="appServerTypeActionGroup">
		<action ref="addNewServer" handler="addNewAppServer" label="添加应用服务"></action>
	</actiongroup>
	
	<action type="push" handler="addMiddleWareToolGroup" desc="Add New Tool Group"
		icon="middle_ware_tool_group_add" id="addMiddleWareToolGroup" confirmInfoKey=""
		label="添加管理工具组" labelKey="">
	</action>
	
	<actiongroup id="middleWareProviderNodeActionGroup">
		<action ref="addMiddleWareToolGroup" />
	</actiongroup>
	
	<action type="push" handler="addMiddleWareTool" desc="Add New Tool "
		icon="middle_ware_tool_add" id="addMiddleWareTool" confirmInfoKey=""
		label="添加管理工具" labelKey="">
	</action>
	
	<actiongroup id="middleWareToolGroupNodeActionGroup">
		<action ref="addMiddleWareToolGroup" />
		<action ref="addMiddleWareTool" />
		<action ref="edit" handler="editMiddleWareToolGroup" icon="edit"/>
		<action ref="delete" handler="deleteMiddleWareToolGroup"/>
	</actiongroup>
	
	<action type="push" handler="editMiddleWareTool" desc="Edit Tool "
		icon="middle_ware_tool_edit" id="editMiddleWareTool" confirmInfoKey=""
		label="编辑管理工具" labelKey="">
	</action>
	
	
	<actiongroup id="middleWareToolNodeActionGroup">
		<action ref="editMiddleWareTool"/>
		<action ref="delete" handler="deleteMiddleWareTool"/>
	</actiongroup>
	
	<action type="push" handler="executeMiddleWareTool" desc="Execute Tool "
		icon="middle_ware_tool_go" id="executeMiddleWareTool" confirmInfoKey=""
		label="执行" labelKey="">
	</action>
	
	<action type="push" handler="stopMiddleWareTool" desc="Execute Tool "
		icon="middle_ware_tool_stop" id="stopMiddleWareTool" confirmInfoKey="是否确认停止?"
		label="终止" labelKey="">
	</action>
	
	<action type="push" handler="cleanMiddleWareToolOutput" desc="Clean Tool Output "
		icon="console_clean" id="cleanMiddleWareToolOutput" confirmInfoKey=""
		label="清空输出" labelKey="">
	</action>
	
	<action type="custom" handler="" desc="" id="showExecuteStatus"
		label="" icon="" labelKey="">
	</action>
	
	<actiongroup id="middleWareToolEditorActionGroup">
	
		<action ref="showExecuteStatus"/>
		<action ref="separator"/>
		<action ref="executeMiddleWareTool"/>
		<action ref="stopMiddleWareTool"/>
		<action ref="cleanMiddleWareToolOutput"/>
		
	</actiongroup>

</actions>