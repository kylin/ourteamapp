<?xml version="1.0" encoding="UTF-8"?>
<listdata>
	
	<select name="database.driverList" type="static">
		<option value="oracle.jdbc.driver.OracleDriver">oracle.jdbc.driver.OracleDriver</option>
		<option value="com.mysql.jdbc.Driver">com.mysql.jdbc.Driver</option>
	</select>
	
	<select name="database.ProviderList" type="static">
		<option value="mysql">mysql</option>
		<option value="oracle">oracle</option>
	</select>
	
	<select name="database.databaseList" type="getDatabaseList"
		dataProperty="businessDomainId" labelProperty="name">
	</select>
	
	<select name="middleWare.providerList" type="getMiddleWareProviderList"
		dataProperty="id" labelProperty="">
	</select>
	
	<select name="middleWare.ScriptTypeList" type="static">
		<option value="ant">ANT脚本</option>
		<option value="shell">Shell处理脚本</option>
		<option value="bat">BAT批处理脚本</option>
		<option value="sql">SQL脚本</option>
	</select>
	
	<select name="middleWare.InputTypeList" type="static">
		<option value="text">输入</option>
		<option value="single">单选</option>
		<option value="multi">多选</option>
		<option value="boolean">是/否</option>
	</select>
	
</listdata>
