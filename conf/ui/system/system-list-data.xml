<?xml version="1.0" encoding="UTF-8"?>
<listdata>
	
	<select name="system.RoleTypeList" type="static">
		<option value="ROLE">角色</option>
		<option value="ROLE_GROUP">角色组</option>
	</select>
	
	<select name="system.systemDomain" type="getSystemDomain"
		dataProperty="id" labelProperty="name">
	</select>
	
	<select name="resourceProperty.dataTypeList" type="static">
		<option value="String">字符</option>
		<option value="Number">数字</option>
		<option value="Date">日期</option>
		<option value="Boolean">布尔</option>
	</select>
	
	<select name="resourceInstance.statusList" type="static">
		<option value="idle">空闲</option>
		<option value="use">占用</option>
		<option value="unavailability">不可用</option>
		<option value="disuse">废弃</option>
	</select>
	
	<select name="bpm.stepTypeList" type="static">
		<option value="GENERAL_TYPE">普通</option>
	</select>
	
	<select name="bpm.stepList" type="getBpmStepDefines"
		dataProperty="id" labelProperty="stepName">
	</select>
	
	<select name="bpm.roleList" type="getBpmRoles"
		dataProperty="id" labelProperty="roleName">
	</select>
	
	<select name="bpm.fieldTypeList" type="static">
		<option value="text_field">文本</option>
		<option value="combo_field">单选</option>
		<option value="check_field">多选</option>
		<option value="date_field">日期</option>
		<option value="time_field">时间</option>
		<option value="group_field">分组</option>
	</select>
	
	<select name="bpm.fieldStatusList" type="static">
		<option value="MUST">必填</option>
		<option value="OPTIONAL">可选</option>
		<option value="READ_ONLY">只读</option>
	</select>
	
</listdata>
