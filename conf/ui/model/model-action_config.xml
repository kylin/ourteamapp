<?xml version='1.0' encoding='UTF-8'?>
<actions>

	<action type="push" handler="addBusinessDomain" desc="addBusinessDomain"
		icon="packfolder_add" id="addBusinessDomain" confirmInfoKey="" label="添加业务域"
		labelKey="action.edit.label">
	</action>

	<actiongroup id="modelBaseViewActionGroup">
		<action ref="addBusinessDomain"></action>
		<action ref="refresh" handler="refreshModelBase"></action>
	</actiongroup>

	<action type="radio" handler="editBusinessDomainGroup" desc="Edit Business Domain Group"
		icon="model_group" id="editBusinessDomainGroup" confirmInfoKey=""
		label="编辑业务域分组..." labelKey="action.edit.label">
	</action>

	<action type="radio" handler="showAllBusinessDomainGroup" desc="Show All Business Domain Group"
		icon="model_group" id="showAllBusinessDomainGroup" confirmInfoKey=""
		label="所有业务域" labelKey="action.edit.label">
	</action>

	<action type="radio" handler="showBusinessDomainGroup" desc="Show Business Domain Group"
		icon="model_group" id="showBusinessDomainGroup" confirmInfoKey=""
		label="${groupName}" labelKey="">
	</action>

	<actiongroup id="modelBaseViewMenuActionGroup">
		<action ref="editBusinessDomainGroup"></action>
		<action ref="showAllBusinessDomainGroup"></action>
	</actiongroup>

	<action type="push" handler="setUnique" desc="set unique" icon="select_all_24"
		id="setUnique" confirmInfoKey="" label="设置唯一" labelKey="action.edit.label">
	</action>

	<action type="push" handler="releaseUnique" desc="release unique"
		icon="un_select_all_24" id="releaseUnique" confirmInfoKey="" label="解除唯一"
		labelKey="action.edit.label">
	</action>

	<actiongroup id="daoPropertyUniqueActionGroup">
		<action ref="setUnique"></action>
		<action ref="releaseUnique"></action>
	</actiongroup>

	<action type="push" handler="createBusinessDomain" desc="create business domain"
		icon="packfolder_add" id="createBusinessDomain" confirmInfoKey=""
		label="新建业务域" labelKey="action.edit.label">
	</action>

	<action type="check" handler="showDaoObject" desc="show DaoObject"
		icon="entity" id="showDaoObject" confirmInfoKey="" label="DAO对象"
		labelKey="action.edit.label">
	</action>

	<action type="check" handler="showBusinessBeanObject" desc="show BusinessBeanObject"
		icon="bean" id="showBusinessBeanObject" confirmInfoKey="" label="DTO对象"
		labelKey="action.edit.label">
	</action>

	<action type="check" handler="showServiceObject" desc="show Service Object"
		icon="service" id="showServiceObject" confirmInfoKey="" label="服务对象"
		labelKey="action.edit.label">
	</action>

	<actiongroup id="businessObjectBeRefrenceFilterActionGroup">
		<action ref="showDaoObject"></action>
		<action ref="showBusinessBeanObject"></action>
		<action ref="showServiceObject"></action>
	</actiongroup>

	<actiongroup id="modelBaseRootActionGroup">
		<action ref="createBusinessDomain"></action>
	</actiongroup>

	<actiongroup id="serviceMethodParameterActionGroup">
		<action ref="addRelation" icon="add"></action>
		<action ref="removeRelation" icon="delete"></action>
		<action ref="editRelation" icon="edit"></action>
		<action ref="moveUp"></action>
		<action ref="moveDown"></action>
	</actiongroup>

	<actiongroup id="customDataTypeActionGroup">
		<action ref="addNew"></action>
		<action ref="edit" icon="edit"></action>
		<action ref="delete"></action>
		<!--<action ref="active"></action> <action ref="deActive"></action> -->
	</actiongroup>

	<action type="push" handler="" desc="add new business package"
		icon="" id="addNewBusinessPackage" confirmInfoKey="" label="新建"
		labelKey="action.edit.label">
		<action type="push" handler="addDaoPackage" desc="add new dao package"
			icon="pack_add" id="addDoaPackage" confirmInfoKey="" label="DAO对象包"
			labelKey="action.edit.label">
		</action>
		<action type="push" handler="addServicePackage" desc="add new service package"
			icon="service_package" id="addServicePackage" confirmInfoKey=""
			label="服务对象包" labelKey="action.edit.label">
		</action>

		<action type="push" handler="addPersistentPackage" desc="add new persistent package"
			icon="packd" id="addPersistentPackage" confirmInfoKey="" label="数据对象包"
			labelKey="action.edit.label">
		</action>
	</action>

	<actiongroup id="businessDomainActionGroup">
		<action ref="addNewBusinessPackage"></action>
		<action ref="delete" handler="deleteBusinessDoamin"></action>
	</actiongroup>

	<action type="push" handler="move" desc="move object" icon="move_to_package"
		id="move" confirmInfoKey="" label="移动" labelKey="action.edit.label">
	</action>

	<action type="push" handler="" desc="add new business object"
		icon="" id="addNewDaoObject" confirmInfoKey="" label="新建"
		labelKey="action.edit.label">
		<action type="push" handler="addDaoObject" desc="add new dao object"
			icon="entity_add" id="addDaoObject" confirmInfoKey="" label="DAO对象"
			labelKey="action.edit.label">
		</action>
	</action>

	<actiongroup id="daoPackageActionGroup">
		<action ref="addNewDaoObject"></action>
		<action ref="delete" handler="deleteBusinessPackage"></action>
		<action ref="move" handler="moveObjectPackage"></action>
	</actiongroup>

	<action type="push" handler="" desc="add new business object"
		icon="" id="addNewServiceObject" confirmInfoKey="" label="新建"
		labelKey="action.edit.label">
		<action type="push" handler="addServiceObject" desc="add new dao object"
			icon="service" id="addServiceObject" confirmInfoKey="" label="服务对象"
			labelKey="action.edit.label">
		</action>
		<action type="push" handler="addBsinessBeanObject" desc="add new bean object"
			icon="class_add" id="addBsinessBeanObject" confirmInfoKey="" label="DTO对象"
			labelKey="action.edit.label">
		</action>
		<action type="push" handler="addEnumObject" desc="add new enum object"
			icon="enum_add" id="addEnumObject" confirmInfoKey="" label="枚举对象"
			labelKey="action.edit.label">
		</action>
	</action>

	<actiongroup id="servicePackageActionGroup">
		<action ref="addNewServiceObject"></action>
		<action ref="delete" handler="deleteBusinessPackage"></action>
		<action ref="move" handler="moveObjectPackage"></action>
	</actiongroup>

	<action type="push" handler="" desc="add new persistent object"
		icon="" id="addNewPersistentObject" confirmInfoKey="" label="新建"
		labelKey="action.edit.label">
		<action type="push" handler="addPersistentObject" desc="add new persistent object"
			icon="table" id="addPersistentObject" confirmInfoKey="" label="数据对象"
			labelKey="action.edit.label">
		</action>
	</action>

	<action type="push" handler="importPersistentObject" desc="import persistent object"
		icon="impotr_table" id="importPersistentObject" confirmInfoKey=""
		label="导入数据对象" labelKey="action.edit.label">
	</action>

	<actiongroup id="persistentPackageActionGroup">
		<action ref="addNewPersistentObject"></action>
		<action ref="importPersistentObject"></action>
		<action ref="delete" handler="deleteBusinessPackage"></action>
		<action ref="move" handler="moveObjectPackage"></action>
	</actiongroup>

	<action type="push" handler="addDaoProperty" desc="add new daoProperty"
		icon="object_property" id="addDaoProperty" confirmInfoKey="" label="添加DAO属性"
		labelKey="action.edit.label">
	</action>

	<action type="push" handler="genDtoObject" desc="gen dto object"
		icon="class_add" id="genDtoObject" confirmInfoKey="" label="导出DTO对象"
		labelKey="action.edit.label">
	</action>

	<actiongroup id="daoObjectActionGroup">
		<action ref="genDtoObject"></action>
		<action ref="addDaoProperty"></action>
		<action ref="delete" handler="deleteBusinessObject"></action>
		<action ref="move" handler="moveObject"></action>
	</actiongroup>

	<action type="push" handler="addBusinessBeanProperty" desc="add new daoProperty"
		icon="field_public" id="addBusinessBeanProperty" confirmInfoKey=""
		label="添加DTO对象属性" labelKey="action.edit.label">
	</action>

	<actiongroup id="beanObjectActionGroup">
		<action ref="addBusinessBeanProperty"></action>
		<action ref="delete" handler="deleteBusinessObject"></action>
		<action ref="move" handler="moveObject"></action>
	</actiongroup>

	<action type="push" handler="addEnumProperty" desc="add new daoProperty"
		icon="enum_property" id="addEnumProperty" confirmInfoKey="" label="添加枚举属性"
		labelKey="action.edit.label">
	</action>

	<actiongroup id="enumObjectActionGroup">
		<action ref="addEnumProperty"></action>
		<action ref="delete" handler="deleteBusinessObject"></action>
		<action ref="move" handler="moveObject"></action>
	</actiongroup>

	<action type="push" handler="addPersistentProperty" desc="add new persistent Property"
		icon="column" id="addPersistentProperty" confirmInfoKey="" label="添加字段"
		labelKey="action.edit.label">
	</action>

	<action type="push" handler="genDaoObject" desc="export dao object"
		icon="entity_add" id="genDaoObject" confirmInfoKey="" label="导出DAO对象"
		labelKey="action.edit.label">
	</action>

	<actiongroup id="persistentObjectActionGroup">
		<action ref="genDaoObject"></action>
		<action ref="addPersistentProperty"></action>
		<action ref="delete" handler="deleteBusinessObject"></action>
		<action ref="move" handler="moveObject"></action>
	</actiongroup>

	<action type="push" handler="addServiceMessthod" desc="add new service method"
		icon="service_method" id="addServiceMessthod" confirmInfoKey="" label="添加服务方法"
		labelKey="action.edit.label">
	</action>

	<actiongroup id="serviceObjectActionGroup">
		<action ref="addServiceMessthod"></action>
		<action ref="delete" handler="deleteBusinessObject"></action>
		<action ref="move" handler="moveObject"></action>
	</actiongroup>

	<actiongroup id="objectPropertyActionGroup">
		<action ref="delete" handler="deleteObjectProperty"></action>
	</actiongroup>

	<actiongroup id="serviceMethodActionGroup">
		<action ref="delete" handler="deleteServiceMethod"></action>
	</actiongroup>

	<action type="push" handler="addNewBusinessTemplate" desc="Add New Business Template"
		icon="template_add" id="addNewBusinessTemplate" confirmInfoKey=""
		label="添加业务模板" labelKey="">
	</action>

	<actiongroup id="businessTypeActionGroup">
		<action ref="addNewBusinessTemplate"></action>
		<action ref="addNew" handler="addNewBusinessTemplateProviderType"
			label="添加模板类型"></action>
	</actiongroup>

	<actiongroup id="editBusinessTypeActionGroup">
		<action ref="edit" handler="editBusinessTemplateProviderType"
			label="编辑模板类型"></action>
	</actiongroup>

	<actiongroup id="genPropertyListActionGroup">
		<action ref="selectAll"></action>
		<action ref="unSelectAll"></action>
	</actiongroup>

</actions>