<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
		   http://www.springframework.org/schema/context
       	   http://www.springframework.org/schema/context/spring-context-2.5.xsd
           http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
           http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd">

		
	<bean id="com.ourteam.modelbase.service.IBusinessDomainService"
		class="com.ourteam.modelbase.service.BusinessDomainServiceImpl" scope="singleton">
		
						
		<property name="businessDomainDao"
			ref="com.ourteam.modelbase.dao.IBusinessDomainDAO" />
								
		<property name="businessDomainGroupDao"
			ref="com.ourteam.modelbase.dao.IBusinessDomainGroupDAO" />
								
		<property name="businessDomainGroupRelateDao"
			ref="com.ourteam.modelbase.dao.IBusinessDomainGroupRelateDAO" />
								
	</bean>
		
	<bean id="com.ourteam.modelbase.service.IBusinessObjectService"
		class="com.ourteam.modelbase.service.BusinessObjectServiceImpl" scope="singleton">
		
						
		<property name="businessObjectDao"
			ref="com.ourteam.modelbase.dao.IBusinessObjectDAO" />
								
		<property name="objectPropertyDao"
			ref="com.ourteam.modelbase.dao.IObjectPropertyDAO" />
								
		<property name="serviceFunctionDao"
			ref="com.ourteam.modelbase.dao.IServiceFunctionDAO" />
								
		<property name="persistentPropertyConstValueDao"
			ref="com.ourteam.modelbase.dao.IPersistentPropertyConstValueDAO" />
								
		<property name="objectRelationDao"
			ref="com.ourteam.modelbase.dao.IObjectRelationDAO" />
								
		<property name="objectRelationAttrDao"
			ref="com.ourteam.modelbase.dao.IObjectRelationAttrDAO" />
								
		<property name="businessObjectActionDao"
			ref="com.ourteam.modelbase.dao.IBusinessObjectActionDAO" />
								
		<property name="serviceParameterDao"
			ref="com.ourteam.modelbase.dao.IServiceParameterDAO" />
								
		<property name="businessPackageDao"
			ref="com.ourteam.modelbase.dao.IBusinessPackageDAO" />
								
	</bean>
		
	<bean id="com.ourteam.modelbase.service.IBusinessPackageService"
		class="com.ourteam.modelbase.service.BusinessPackageServiceImpl" scope="singleton">
		
						
		<property name="businessPackageDao"
			ref="com.ourteam.modelbase.dao.IBusinessPackageDAO" />
								
	</bean>
		
	<bean id="com.ourteam.modelbase.service.IBusinessTemplateService"
		class="com.ourteam.modelbase.service.BusinessTemplateServiceImpl" scope="singleton">
		
						
		<property name="velocityTemplateDao"
			ref="com.ourteam.modelbase.dao.IVelocityTemplateDAO" />
								
		<property name="businessTemplateDao"
			ref="com.ourteam.modelbase.dao.IBusinessTemplateDAO" />
								
		<property name="businessDomainService"
			ref="com.ourteam.modelbase.service.IBusinessDomainService" />		
								
		<property name="businessObjectService"
			ref="com.ourteam.modelbase.service.IBusinessObjectService" />		
								
		<property name="businessPackageService"
			ref="com.ourteam.modelbase.service.IBusinessPackageService" />		
								
		<property name="businessTemplateProviderTypeDao"
			ref="com.ourteam.modelbase.dao.IBusinessTemplateProviderTypeDAO" />
								
	</bean>
	
</beans>