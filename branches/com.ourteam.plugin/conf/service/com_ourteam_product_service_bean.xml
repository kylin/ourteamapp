<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
		   http://www.springframework.org/schema/context
       	   http://www.springframework.org/schema/context/spring-context-2.5.xsd
           http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
           http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd">

		
	<bean id="com.ourteam.product.service.IProductCatalogService"
		class="com.ourteam.product.service.ProductCatalogServiceImpl" scope="singleton">
		
						
		<property name="productCatalogDao"
			ref="com.ourteam.product.dao.IProductCatalogDAO" />
								
		<property name="productDefineDao"
			ref="com.ourteam.product.dao.IProductDefineDAO" />
								
	</bean>
		
	<bean id="com.ourteam.product.service.IProductFunctionService"
		class="com.ourteam.product.service.ProductFunctionServiceImpl" scope="singleton">
		
						
		<property name="productFunctionDao"
			ref="com.ourteam.product.dao.IProductFunctionDAO" />
								
		<property name="productFunctionVersionRelateDao"
			ref="com.ourteam.product.dao.IProductFunctionVersionRelateDAO" />
								
	</bean>
		
	<bean id="com.ourteam.product.service.IProductService"
		class="com.ourteam.product.service.ProductServiceImpl" scope="singleton">
		
						
		<property name="productDefineDao"
			ref="com.ourteam.product.dao.IProductDefineDAO" />
								
		<property name="productTeamRelateDao"
			ref="com.ourteam.product.dao.IProductTeamRelateDAO" />
								
		<property name="productGlossaryDao"
			ref="com.ourteam.product.dao.IProductGlossaryDAO" />
								
		<property name="productGlossaryChangLogDao"
			ref="com.ourteam.product.dao.IProductGlossaryChangLogDAO" />
								
	</bean>
		
	<bean id="com.ourteam.product.service.IProductVersionService"
		class="com.ourteam.product.service.ProductVersionServiceImpl" scope="singleton">
		
						
		<property name="productDefineDao"
			ref="com.ourteam.product.dao.IProductDefineDAO" />
								
		<property name="productVersionDao"
			ref="com.ourteam.product.dao.IProductVersionDAO" />
								
	</bean>
	
</beans>