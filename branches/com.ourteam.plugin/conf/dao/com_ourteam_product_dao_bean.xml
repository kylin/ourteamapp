<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
		   http://www.springframework.org/schema/context
       	   http://www.springframework.org/schema/context/spring-context-2.5.xsd
           http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
           http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd">

			
		
	<bean id="com.ourteam.product.dao.IProductCatalogDAO"
		class="com.ourteam.product.dao.impl.ProductCatalogDAO" scope="singleton">
				
				
						
		<property name="dataSource" ref="dataSource"/>
				
						
	</bean>
					
		
	<bean id="com.ourteam.product.dao.IProductDefineDAO"
		class="com.ourteam.product.dao.impl.ProductDefineDAO" scope="singleton">
				
				
						
		<property name="dataSource" ref="dataSource"/>
				
						
		<property name="productVersion" ref="com.ourteam.product.dao.IProductVersionDAO"/>
								
	</bean>
					
		
	<bean id="com.ourteam.product.dao.IProductFunctionDAO"
		class="com.ourteam.product.dao.impl.ProductFunctionDAO" scope="singleton">
				
				
						
		<property name="dataSource" ref="dataSource"/>
				
						
	</bean>
					
		
	<bean id="com.ourteam.product.dao.IProductFunctionVersionRelateDAO"
		class="com.ourteam.product.dao.impl.ProductFunctionVersionRelateDAO" scope="singleton">
				
				
						
		<property name="dataSource" ref="dataSource"/>
				
						
		<property name="productFunction" ref="com.ourteam.product.dao.IProductFunctionDAO"/>
								
		<property name="productVersion" ref="com.ourteam.product.dao.IProductVersionDAO"/>
								
	</bean>
					
		
	<bean id="com.ourteam.product.dao.IProductGlossaryDAO"
		class="com.ourteam.product.dao.impl.ProductGlossaryDAO" scope="singleton">
				
				
						
		<property name="dataSource" ref="dataSource"/>
				
																		
	</bean>
					
		
	<bean id="com.ourteam.product.dao.IProductGlossaryChangLogDAO"
		class="com.ourteam.product.dao.impl.ProductGlossaryChangLogDAO" scope="singleton">
				
				
						
		<property name="dataSource" ref="dataSource"/>
				
						
	</bean>
					
		
	<bean id="com.ourteam.product.dao.IProductTeamRelateDAO"
		class="com.ourteam.product.dao.impl.ProductTeamRelateDAO" scope="singleton">
				
				
						
		<property name="dataSource" ref="dataSource"/>
				
																		
	</bean>
					
		
	<bean id="com.ourteam.product.dao.IProductVersionDAO"
		class="com.ourteam.product.dao.impl.ProductVersionDAO" scope="singleton">
				
				
						
		<property name="dataSource" ref="dataSource"/>
				
						
	</bean>
			
</beans>