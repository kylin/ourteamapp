<?xml version="1.0" encoding="UTF-8"?>
<listdata>
	
	<select name="JDK.versionList" type="static">
		<option value="jdk1.4">JDK 1.4</option>
		<option value="jdk1.5">JDK 1.5</option>
		<option value="jdk1.6">JDK 1.6</option>
		<option value="jdk1.7">JDK 1.7</option>
	</select>
	
	<select name="ORM.typeList" type="static">
		<option value="SpringDAO">Spring DAO</option>
		<option value="ibitas2.3">iBitas 2.3.X</option>
		<option value="jpaHibernate">JPA Hibernate</option>
	</select>
	
	<select name="workspaceConfig.daoProviderType" type="getDAOProviderType"
		dataProperty="id" labelProperty="providerName">
	</select>
	
	<select name="workspaceConfig.scmProviderType" type="getSCMService"
		dataProperty="id" labelProperty="name">
	</select>
	
	<select name="cvs.ConnectTypeList" type="static">
		<option value="ext">extssh</option>
		<option value="pserver">pserver</option>
		<option value="pserverssh2">pserverssh2</option>
	</select>
	
	<select name="svn.ConnectTypeList" type="static">
		<option value="https">https</option>
		<option value="svn+ssh">svn+ssh</option>
		<option value="http">http</option>
	</select>
	
	<select name="cms.resourceVersionList" type="getResourceVersionList"
		dataProperty="revision" labelProperty="">
	</select>
	
	<select name="Form.propertyType" type="static">
		<option value="text">文本框</option>
		<option value="radiobutton">单选框</option>
		<option value="checkbox">复选框</option>
		<option value="combolist">下拉列表</option>
		<option value="combodate">日期选择</option>
		<option value="group">面板</option>
		<option value="ref">引用</option>
	</select>
	
	<select name="Form.layoutProperty" type="static">
		<option value=""></option>
		<option value="verticalAlignment">verticalAlignment</option>
		<option value="horizontalAlignment">horizontalAlignment</option>
		<option value="widthHint">widthHint</option>
		<option value="heightHint">heightHint</option>
		<option value="horizontalIndent">horizontalIndent</option>
		<option value="verticalIndent">verticalIndent</option>
		<option value="horizontalSpan">horizontalSpan</option>
		<option value="verticalSpan">verticalSpan</option>
		<option value="grabExcessHorizontalSpace">grabExcessHorizontalSpace</option>
		<option value="grabExcessVerticalSpace">grabExcessVerticalSpace</option>
		<option value="minimumWidth">minimumWidth</option>
		<option value="minimumHeight">minimumHeight</option>
		<option value="exclude">exclude</option>
	</select>
	
</listdata>
