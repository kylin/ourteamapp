	</bean>
					
		
	<bean id="com.ourteam.middleware.dao.IMiddleWareToolInputOptionDAO"
		class="com.ourteam.middleware.dao.impl.MiddleWareToolInputOptionDAO" scope="singleton">
				
				
						
