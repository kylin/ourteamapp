	</bean>
					
		
	<bean id="com.ourteam.middleware.dao.IMiddleWareToolDAO"
		class="com.ourteam.middleware.dao.impl.MiddleWareToolDAO" scope="singleton">
				
				
						
		<property name="dataSource" ref="dataSource"/>
				
																		
	</bean>
					
		
	<bean id="com.ourteam.middleware.dao.IMiddleWareToolGroupDAO"
		class="com.ourteam.middleware.dao.impl.MiddleWareToolGroupDAO" scope="singleton">
				
				
						
		<property name="dataSource" ref="dataSource"/>
				
						
	</bean>
					
		
	<bean id="com.ourteam.middleware.dao.IMiddleWareToolInputDAO"
		class="com.ourteam.middleware.dao.impl.MiddleWareToolInputDAO" scope="singleton">
				
				
						
		<property name="dataSource" ref="dataSource"/>
				
						
